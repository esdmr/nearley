{
  "name": "nearley",
  "version": "2.20.1",
  "description": "Simple, fast, powerful parser toolkit for JavaScript.",
  "imports": {
    "#nearley": "./lib/runtime/index.js"
  },
  "exports": {
    ".": {
      "types": "./types/runtime/index.js",
      "import": "./lib/runtime/index.js"
    },
    "./*": {
      "types": "./types/runtime/*.d.ts",
      "import": "./lib/runtime/*.js"
    },
    "./package.json": "./package.json",
    "./compiler/*": {
      "node": {
        "types": "./types/compiler/*.d.ts",
        "import": "./lib/compiler/*.js"
      }
    }
  },
  "type": "module",
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "commander": "^11.0.0",
    "moo": "^0.5.2",
    "@types/moo": "^0.5.5"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "bin": {
    "nearleyc": "lib/bin/nearleyc.js"
  },
  "keywords": [
    "parser",
    "parse",
    "generator",
    "compiler",
    "compile",
    "grammar",
    "language"
  ],
  "scripts": {
    "bootstrap": "./lib/bin/nearleyc.js -O lib/compiler/nearley-language-bootstrapped.ne -o tmp && mv tmp lib/compiler/nearley-language-bootstrapped.js && echo bootstrapped ok"
  },
  "author": "Hardmath123",
  "contributors": "https://github.com/Hardmath123/nearley/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hardmath123/nearley.git"
  },
  "funding": {
    "type": "individual",
    "url": "https://nearley.js.org/#give-to-nearley"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.4",
    "@tsconfig/node18": "^18.2.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "18",
    "typescript": "^5.1.6",
    "xo": "^0.55.0"
  }
}
